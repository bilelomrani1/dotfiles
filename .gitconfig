# Local/private config goes in the include
[user]
        name = Bilel Omrani
        email = bilelomrani1@gmail.com
[hub]
        protocol = https
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = code --wait
[apply]
        whitespace = nowarn
[mergetool]
        keepBackup = false
[diff]
        mnemonicPrefix = true
        renames = true
        tool = vscode
[difftool "vscode"]
        cmd = code --wait --diff $LOCAL $REMOTE
[help]
        autocorrect = 1
[push]
        default = simple
[credential]
	helper = osxkeychain
	helper = /usr/local/share/gcm-core/git-credential-manager-core
